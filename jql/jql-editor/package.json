{
	"name": "@atlaskit/jql-editor",
	"version": "5.0.7",
	"description": "This package allows consumers to render an advanced JQL editor component to enable autocomplete-assisted authoring and validation of JQL queries.",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Jira Platform: Search Experience / Empanada",
		"website": {
			"name": "JQL Editor",
			"category": "JQL"
		},
		"runReact18": true
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"af:exports": {
		".": "./src/index.ts",
		"./i18n": "./src/i18n/index.ts"
	},
	"exports": {
		".": "./src/index.ts",
		"./i18n": "./src/i18n/index.ts",
		"./i18n/*": "./src/i18n/*.ts"
	},
	"dependencies": {
		"@atlaskit/analytics-gas-types": "workspace:^",
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/avatar": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/editor-prosemirror": "7.0.0",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/jql-ast": "workspace:^",
		"@atlaskit/jql-autocomplete": "workspace:^",
		"@atlaskit/jql-editor-common": "workspace:^",
		"@atlaskit/jql-parser": "workspace:^",
		"@atlaskit/legacy-custom-icons": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/spinner": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/tooltip": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.0.0",
		"@react-loosely-lazy/manifest": "^1.0.0",
		"antlr4ts": "0.5.0-alpha.4",
		"assert": "^2.0.0",
		"lodash": "^4.17.21",
		"react-intl-next": "npm:react-intl@^5.18.1",
		"react-loosely-lazy": "^1.0.0",
		"react-magnetic-di": "^3.1.4",
		"react-sweet-state": "^2.6.5",
		"rxjs": "^5.5.0",
		"util": "^0.12.4",
		"uuid": "^3.1.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/integration-testing": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/jql-editor-autocomplete-rest": "workspace:^",
		"@atlaskit/visual-regression": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@storybook/addon-actions": "^6.4.0",
		"@storybook/addon-knobs": "^6.4.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"typescript": "~5.4.2",
		"wait-for-expect": "^1.2.0"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color",
				"spacing"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": [
				"no-deprecated-imports"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"stricter": {
		"no-unused-dependencies": {
			"exclude": [
				"assert",
				"util"
			]
		}
	},
	"platform-feature-flags": {
		"platform-component-visual-refresh": {
			"type": "boolean"
		},
		"add_nin_press_interactions": {
			"type": "boolean"
		},
		"jql_editor_a11y": {
			"type": "boolean"
		}
	}
}
