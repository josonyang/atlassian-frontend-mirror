{
  "name": "@atlaskit/editor-plugin-base",
  "version": "0.2.3",
  "description": "Base plugin for @atlaskit/editor-core",
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "atlassian": {
    "team": "Editor",
    "singleton": true,
    "releaseModel": "continuous"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "af:exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@atlaskit/adf-schema": "^32.0.0",
    "@atlaskit/editor-common": "^76.16.0",
    "@atlaskit/editor-plugin-feature-flags": "^1.0.1",
    "@atlaskit/editor-prosemirror": "1.1.0",
    "@atlaskit/platform-feature-flags": "^0.2.0",
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@atlaskit/editor-plugin-analytics": "^0.2.0",
    "@atlaskit/editor-plugin-content-insertion": "^0.1.0",
    "@atlaskit/editor-plugin-emoji": "^0.4.0",
    "@atlaskit/editor-plugin-guideline": "^0.5.0",
    "@atlaskit/editor-plugin-selection": "^0.1.0",
    "@atlaskit/editor-plugin-table": "^5.3.0",
    "@atlaskit/editor-plugin-type-ahead": "^0.7.0",
    "@atlaskit/editor-plugin-width": "^0.2.0",
    "@atlaskit/util-data-test": "^17.8.0",
    "@atlaskit/webdriver-runner": "*",
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@testing-library/react": "^12.1.5",
    "raf-stub": "^2.0.1"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "analytics": [
        "analytics-next"
      ],
      "design-tokens": [
        "color"
      ],
      "theming": [
        "react-context"
      ],
      "ui-components": [
        "lite-mode"
      ],
      "deprecation": [
        "no-deprecated-imports"
      ],
      "styling": [
        "static",
        "emotion"
      ]
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0",
  "platform-feature-flags": {
    "platform.editor.disable-chrome-88-selection-fix_uk53m": {
      "type": "boolean"
    }
  }
}
