{
	"name": "@atlaskit/editor-plugin-floating-toolbar",
	"version": "6.1.0",
	"description": "Floating toolbar plugin for @atlaskit/editor-core",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
