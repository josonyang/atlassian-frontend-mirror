		},
		"platform_editor_merge_unconfirmed_steps": {
			"type": "boolean"
		},
		"platform_editor_offline_conflict_resolution": {
			"type": "boolean"
		},
		"platform_editor_selection_without_left_shift": {
			"type": "boolean"
		}
	}
