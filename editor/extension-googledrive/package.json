{
	"name": "@atlaskit/editor-extension-googledrive",
	"version": "4.1.0",
	"description": "An Atlassian editor extension to add a native Google Drive picker",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"url": "https://bitbucket.org/atlassian/atlassian-frontend.git"
	},
	"author": "Atlassian",
	"license": "Apache-2.0",
	"atlaskit:src": "src/index.ts",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"main": "dist/cjs/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlassian": {
		"team": "trello-ecosystem"
	},
	"exports": {
		".": "./src/index.ts"
	},
	"dependencies": {
		"@atlaskit/adf-schema": "root:*",
		"@atlaskit/adf-utils": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*",
		"@types/gapi": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/editor-common": "workspace:^",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"techstack": {
		"@repo/internal": {
			"design-tokens": [
				"color"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"compiled"
			]
		}
	}
}
