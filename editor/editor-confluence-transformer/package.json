{
	"name": "@atlaskit/editor-confluence-transformer",
	"version": "12.2.1",
	"description": "Editor Confluence Transformer",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./collapse-whitespace": "./src/collapse-whitespace.ts",
		"./content-wrapper": "./src/content-wrapper.ts",
		"./encode": "./src/encode.ts",
		"./encode-cxhtml": "./src/encode-cxhtml.ts",
		"./languageMap": "./src/languageMap.ts",
		"./parse": "./src/parse.ts",
		"./parse-cxhtml": "./src/parse-cxhtml.ts",
		"./types": "./src/types.ts",
		"./utils": "./src/utils.ts"
	},
	"atlassian": {
		"team": "Editor: Collaboration",
		"website": {
			"name": "Editor Confluence Transformer"
		}
	},
	"dependencies": {
		"@atlaskit/adf-schema": "root:*",
		"@atlaskit/editor-prosemirror": "root:*",
		"@atlaskit/editor-shared-styles": "workspace:^",
		"@babel/runtime": "root:*",
		"collapse-whitespace": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/editor-common": "workspace:^",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@atlaskit/editor-json-transformer": "workspace:^",
		"@atlaskit/editor-plugins": "workspace:^",
		"@atlaskit/editor-test-helpers": "workspace:^",
		"@emotion/react": "root:*",
		"pretty-data": "root:*"
	},
	"techstack": {
		"@repo/internal": {
			"design-tokens": [
				"color"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion"
			]
		}
	}
}
