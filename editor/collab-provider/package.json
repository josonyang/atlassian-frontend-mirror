		"./types": "./src/types.ts",
		"./version-wrapper": "./src/version-wrapper.ts"
	},
	"dependencies": {
		"@atlaskit/adf-utils": "^19.18.0",
		"@atlaskit/analytics-gas-types": "^5.1.0",
