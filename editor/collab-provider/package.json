    "@atlaskit/editor-json-transformer": "^8.10.0",
    "@atlaskit/editor-prosemirror": "3.0.0",
    "@atlaskit/platform-feature-flags": "^0.2.0",
    "@atlaskit/prosemirror-collab": "^0.2.0",
    "@atlaskit/ufo": "^0.2.0",
    "@atlaskit/util-service-support": "^6.2.0",
