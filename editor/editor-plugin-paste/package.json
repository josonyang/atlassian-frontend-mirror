{
	"name": "@atlaskit/editor-plugin-paste",
	"version": "3.1.2",
	"description": "Paste plugin for @atlaskit/editor-core",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Editor: Lego",
		"singleton": true,
		"runReact18": true
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/index.ts",
	"af:exports": {
		".": "./src/index.ts"
	},
	"exports": {
		".": "./src/index.ts"
	},
	"dependencies": {
		"@atlaskit/adf-schema": "^47.6.0",
		"@atlaskit/code": "workspace:*",
		"@atlaskit/editor-common": "workspace:*",
		"@atlaskit/editor-markdown-transformer": "workspace:*",
		"@atlaskit/editor-plugin-analytics": "workspace:*",
		"@atlaskit/editor-plugin-annotation": "workspace:*",
		"@atlaskit/editor-plugin-better-type-history": "workspace:*",
		"@atlaskit/editor-plugin-card": "workspace:*",
		"@atlaskit/editor-plugin-feature-flags": "workspace:*",
		"@atlaskit/editor-plugin-list": "workspace:*",
		"@atlaskit/editor-plugin-media": "workspace:*",
		"@atlaskit/editor-plugin-mentions": "workspace:*",
		"@atlaskit/editor-prosemirror": "7.0.0",
		"@atlaskit/editor-tables": "workspace:*",
		"@atlaskit/media-client": "workspace:*",
		"@atlaskit/media-common": "workspace:*",
		"@atlaskit/platform-feature-flags": "workspace:*",
		"@atlaskit/tmp-editor-statsig": "workspace:*",
		"@babel/runtime": "^7.0.0",
		"lodash": "^4.17.21",
		"uuid": "^3.1.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/visual-regression": "workspace:*",
		"@atlaskit/editor-plugin-block-type": "workspace:*",
		"@atlaskit/editor-plugin-history": "workspace:*",
		"@atlaskit/editor-plugin-type-ahead": "workspace:*",
		"@atlaskit/ssr": "workspace:*",
		"@atlaskit/visual-regression": "workspace:*",
		"@testing-library/react": "^13.4.0",
		"typescript": "~5.4.2",
		"wait-for-expect": "^1.2.0"
	},
	"techstack": {
		"@atlassian/frontend": {
			"code-structure": [
				"editor-plugin"
			],
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion",
				"emotion"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"platform-feature-flags": {
		"platform_editor_media_extended_resize_experience": {
			"type": "boolean"
		},
		"editor_inline_comments_paste_insert_nodes": {
			"type": "boolean"
		},
		"platform_editor_nest_nested_expand_in_expand_jira": {
			"type": "boolean"
		},
		"platform_editor_use_nested_table_pm_nodes": {
			"type": "boolean"
		},
		"platform_editor_vs_code_block_paste": {
			"type": "boolean"
		},
		"platform_editor_advanced_layouts_post_fix_patch_2": {
			"type": "boolean"
		},
		"platform_editor_fix_captions_on_copy": {
			"type": "boolean"
		},
		"platform_editor_fix_paste_action_item_in_list": {
			"type": "boolean"
		},
		"platform_editor_legacy_content_macro": {
			"type": "boolean"
		},
		"platform_editor_macroid_reset_for_ext_on_paste": {
			"type": "boolean"
		},
		"platform_editor_legacy_content_macro_insert": {
			"type": "boolean"
		},
		"platform_editor_track_node_types": {
			"type": "boolean"
		}
	}
}
