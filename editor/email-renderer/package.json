	"scripts": {
		"ak-postbuild": "ts-node --project ./tsconfig.ts-node.json ./build/ts-to-css.ts",
		"build:images": "ts-node --files --project ./tsconfig.ts-node.json ./src/static/generator/generate.ts"
	},
	"dependencies": {
		"@atlaskit/adf-schema": "^40.3.0",
