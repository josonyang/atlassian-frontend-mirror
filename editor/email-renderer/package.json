		"mkdirp": "^0.5.1",
		"mockdate": "^3.0.5",
		"react": "^16.8.0",
		"rimraf": "^2.6.3",
		"svgexport": "^0.4.0",
		"ts-node": "^10.9.1",
		"typescript": "~5.4.2"
