{
	"name": "@atlaskit/editor-plugin-quick-insert",
	"version": "1.7.1",
	"description": "Quick insert plugin for @atlaskit/editor-core",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
