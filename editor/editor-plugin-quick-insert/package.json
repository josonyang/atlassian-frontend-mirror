		"@atlaskit/icon": "^22.27.0",
		"@atlaskit/modal-dialog": "^12.18.0",
		"@atlaskit/theme": "^14.0.0",
		"@atlaskit/tokens": "^2.4.0",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ~18.2.0",
