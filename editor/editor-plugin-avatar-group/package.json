{
	"name": "@atlaskit/editor-plugin-avatar-group",
	"version": "7.0.1",
	"description": "Avatar Group plugin for @atlaskit/editor-core.",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Editor: Collaboration",
		"website": {
			"name": "EditorPluginAvatarGroup",
			"category": "Components"
		}
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"dependencies": {
		"@atlaskit/avatar-group": "workspace:^",
		"@atlaskit/editor-plugin-analytics": "workspace:^",
		"@atlaskit/editor-plugin-collab-edit": "workspace:^",
		"@atlaskit/editor-plugin-feature-flags": "workspace:^",
		"@atlaskit/editor-plugin-primary-toolbar": "workspace:^",
		"@atlaskit/editor-prosemirror": "root:*",
		"@atlaskit/editor-shared-styles": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tmp-editor-statsig": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@emotion/react": "root:*",
		"memoize-one": "root:*",
		"react-intl-next": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/editor-common": "workspace:^",
		"react": "^18.2.0"
	},
	"techstack": {
		"@atlassian/frontend": {
			"code-structure": [
				"editor-plugin"
			],
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"analytics": [
				"analytics-next"
			],
			"design-tokens": [
				"color"
			],
			"theming": [
				"react-context"
			],
			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion",
				"compiled"
			],
			"imports": [
				"import-no-extraneous-disable-for-examples-and-docs"
			]
		}
	},
	"stricter": {
		"no-unused-dependencies": {
			"checkDevDependencies": true
		}
	}
}
