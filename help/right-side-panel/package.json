	"dependencies": {
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*",
		"exenv": "root:*",
		"react-transition-group": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-intl-next": "*"
	},
	"devDependencies": {
		"@atlaskit/page": "workspace:^",
		"@testing-library/react": "root:*",
		"@types/exenv": "root:*",
		"@types/react-transition-group": "root:*",
		"enzyme": "root:*",
		"react-test-renderer": "root:*"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui",
		"core",
		"panel"
	],
	"techstack": {
		"@repo/internal": {
			"design-tokens": [
				"color",
				"spacing"
			],
			"styling": [
				"compiled"
			]
		}
	}
