{
	"name": "@atlaskit/icon-file-type",
	"version": "7.0.5",
	"description": "A file type icon is used to represent specific types of content used across Atlassian products.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"atlaskit:src": "src/index.ts",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlassian": {
		"team": "Media Exif",
		"website": {
			"name": "Icon File Type"
		}
	},
	"af:exports": {
		"./metadata": "./src/entry-points/metadata.ts",
		"./glyph": "./glyph",
		".": "./src/index.ts"
	},
	"exports": {
		"./metadata": "./src/entry-points/metadata.ts",
		"./glyph/*": {
			"types": "./glyph/*.d.ts",
			"default": "./glyph/*.js"
		},
		".": "./src/index.ts"
	},
	"scripts": {
		"build-glyphs": "ts-node --project ../../../tsconfig.node.json ./build/index.ts"
	},
	"dependencies": {
		"@atlaskit/icon": "workspace:^",
		"@babel/runtime": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/icon-build-process": "workspace:^",
		"@emotion/styled": "root:*",
		"enzyme": "root:*",
		"fs-extra": "root:*",
		"jscodeshift": "root:*",
		"pkg-dir": "root:*",
		"react-dom": "root:*",
		"ts-node": "root:*"
	},
	"keywords": [
		"atlaskit",
		"ui"
	],
	"techstack": {
		"@repo/internal": {
			"design-tokens": [
				"color",
				"spacing"
			]
		}
	}
}
