	"dependencies": {
		"@atlaskit/media-common": "workspace:^",
		"@babel/runtime": "root:*",
		"immer": "root:*",
		"zustand": "root:*"
	},
	"devDependencies": {
		"typescript": "root:*",
		"wait-for-expect": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": [
				"file-and-folder-level"
			]
		}
	}
