{
	"name": "@atlaskit/media-client",
	"version": "35.1.0",
	"description": "Media API Web Client Library",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/index.ts",
	"af:exports": {
		"./constants": "./src/constants.ts",
		"./test-helpers": "./src/test-helpers/index.ts",
		".": "./src/index.ts"
	},
	"exports": {
		"./constants": "./src/constants.ts",
		"./test-helpers": "./src/test-helpers/index.ts",
		".": "./src/index.ts"
	},
	"atlassian": {
		"team": "Media Exif",
		"website": {
			"name": "Media Client"
		}
	},
	"dependencies": {
		"@atlaskit/chunkinator": "workspace:^",
		"@atlaskit/media-common": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@babel/runtime": "root:*",
		"dataloader": "root:*",
		"deep-equal": "root:*",
		"eventemitter2": "root:*",
		"js-sha256": "root:*",
		"lru_map": "root:*",
		"rusha": "root:*",
		"rxjs": "root:*",
		"uuid": "root:*",
		"uuid-validate": "root:*",
		"xstate": "root:*"
	},
	"peerDependencies": {
		"@atlaskit/media-core": "workspace:^",
		"@atlaskit/media-state": "workspace:^"
	},
	"devDependencies": {
		"@atlaskit/media-core": "workspace:^",
		"@atlaskit/media-state": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@emotion/react": "root:*",
		"@types/deep-equal": "root:*",
		"@types/uuid-validate": "root:*",
		"fetch-mock": "root:*",
		"react": "root:*",
		"react-dom": "root:*",
		"typescript": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"circular-dependencies": [
				"file-level"
			]
		},
		"@repo/internal": {
			"design-tokens": [
				"color",
				"spacing"
			]
		}
	},
	"platform-feature-flags": {
		"platform_media_cdn_single_host": {
			"type": "boolean"
		},
		"platform_media_cdn_delivery": {
			"type": "boolean"
		}
	}
}
