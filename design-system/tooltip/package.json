{
	"name": "@atlaskit/tooltip",
	"version": "20.0.5",
	"description": "A tooltip is a floating, non-actionable label used to explain a user interface element or feature.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"atlassian": {
		"team": "Design System Team",
		"runReact18": true,
		"website": {
			"name": "Tooltip",
			"category": "Overlays and layering"
		}
	},
	"af:exports": {
		".": "./src/index.ts",
		"./Tooltip": "./src/tooltip.tsx",
		"./TooltipContainer": "./src/tooltip-container.tsx",
		"./TooltipPrimitive": "./src/tooltip-primitive.tsx",
		"./types": "./src/types.tsx",
		"./utilities": "./src/utilities.ts"
	},
	"exports": {
		".": "./src/index.ts",
		"./Tooltip": "./src/tooltip.tsx",
		"./TooltipContainer": "./src/tooltip-container.tsx",
		"./TooltipPrimitive": "./src/tooltip-primitive.tsx",
		"./types": "./src/types.tsx",
		"./utilities": "./src/utilities.ts"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/motion": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/popper": "workspace:^",
		"@atlaskit/portal": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.3",
		"@emotion/react": "^11.7.1",
		"bind-event-listener": "^3.0.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "^13.4.0",
		"react-dom": "^18.2.0",
		"tiny-invariant": "^1.2.0",
		"typescript": "~5.4.2"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion",
				"static"
			]
		}
	},
	"homepage": "https://atlassian.design/components/tooltip/",
	"platform-feature-flags": {
		"platform_dst_open_layer_observer_close_layers": {
			"type": "boolean"
		}
	}
}
