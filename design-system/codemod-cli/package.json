		"./cli": "./src/cli.ts",
		"./filepath": "./src/filepath.ts",
		"./main": "./src/main.ts",
		"./sinceRef": "./src/sinceRef.ts",
		"./transforms": "./src/transforms.ts",
		"./types": "./src/types.ts",
		"./utils": "./src/utils.ts"
	},
	"atlassian": {
