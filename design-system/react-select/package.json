{
	"name": "@atlaskit/react-select",
	"version": "2.4.4",
	"description": "A forked version of react-select to only be used in atlaskit/select",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Design System Team",
		"runReact18": true
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./base": "./src/select.tsx",
		"./async": "./src/async.tsx",
		"./creatable": "./src/creatable.tsx",
		"./async-creatable": "./src/async-creatable.tsx",
		"./types": "./src/types.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./base": "./src/select.tsx",
		"./async": "./src/async.tsx",
		"./creatable": "./src/creatable.tsx",
		"./async-creatable": "./src/async-creatable.tsx",
		"./types": "./src/types.tsx"
	},
	"dependencies": {
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/spinner": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.3",
		"@emotion/react": "^11.7.1",
		"@floating-ui/dom": "^1.0.1",
		"memoize-one": "^6.0.0",
		"use-isomorphic-layout-effect": "^1.1.2"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@atlaskit/checkbox": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"jest-in-case": "^1.0.2",
		"typescript": "~5.4.2"
	},
	"platform-feature-flags": {
		"custom-interactive-elements-not-keyboard-focusable": {
			"type": "boolean"
		},
		"design_system_select-a11y-improvement": {
			"type": "boolean"
		},
		"platform-component-visual-refresh": {
			"type": "boolean"
		},
		"platform_design_system_team_safari_input_fix": {
			"type": "boolean"
		},
		"design-system-select-fix-placement": {
			"type": "boolean"
		},
		"platform_dst_layer_observer_select": {
			"type": "boolean"
		},
		"compiled-react-select": {
			"type": "boolean"
		}
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": "file-and-folder-level"
		},
		"@repo/internal": {
			"design-system": "v1",
			"dom-events": "use-bind-event-listener",
			"analytics": "analytics-next",
			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion",
				"compiled"
			]
		}
	},
	"homepage": "https://atlassian.design/components",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	}
