{
	"name": "@atlaskit/react-select",
	"version": "1.6.0",
	"description": "A forked version of react-select to only be used in atlaskit/select",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Design System Team",
		"runReact18": true
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./base": "./src/select.tsx",
		"./async": "./src/async.tsx",
		"./creatable": "./src/creatable.tsx",
		"./async-creatable": "./src/async-creatable.tsx",
		"./types": "./src/types.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./base": "./src/select.tsx",
		"./async": "./src/async.tsx",
		"./creatable": "./src/creatable.tsx",
		"./async-creatable": "./src/async-creatable.tsx",
		"./types": "./src/types.tsx"
	},
	"dependencies": {
		"@atlaskit/ds-lib": "^3.3.0",
		"@atlaskit/icon": "^23.2.0",
		"@atlaskit/platform-feature-flags": "^0.3.0",
		"@atlaskit/primitives": "^13.3.0",
		"@atlaskit/spinner": "^16.3.0",
		"@atlaskit/tokens": "^2.5.0",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1",
		"@floating-ui/dom": "^1.0.1",
		"memoize-one": "^6.0.0",
		"use-isomorphic-layout-effect": "^1.1.2"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.2.0",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.2.0"
	},
	"devDependencies": {
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/ssr": "*",
		"@atlaskit/visual-regression": "*",
		"@testing-library/react": "^12.1.5",
		"@testing-library/user-event": "^14.4.3",
		"jest-in-case": "^1.0.2",
		"react-intl-next": "npm:react-intl@^5.18.1",
		"typescript": "~5.4.2",
		"wait-for-expect": "^1.2.0"
	},
	"platform-feature-flags": {
		"design_system_select-a11y-improvement": {
			"type": "boolean"
		},
		"platform-component-visual-refresh": {
			"type": "boolean"
		},
		"platform_design_system_team_safari_input_fix": {
			"type": "boolean"
		},
		"design-system-select-fix-placement": {
			"type": "boolean"
		},
		"platform-visual-refresh-icons-legacy-facade": {
			"type": "boolean"
		}
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": [
				"atlassian-conventions"
			],
			"circular-dependencies": "file-and-folder-level"
		},
		"@repo/internal": {
			"design-system": "v1",
			"dom-events": "use-bind-event-listener",
			"analytics": "analytics-next",
			"ui-components": [
				"lite-mode"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"homepage": "https://atlassian.design/components",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	}
