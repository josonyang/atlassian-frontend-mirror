		"./loading-button": "./src/entry-points/loading-button.tsx",
		"./standard-button": "./src/entry-points/standard-button.tsx",
		"./new": "./src/entry-points/new.tsx",
		"./types": "./src/entry-points/types.tsx",
		".": "./src/index.tsx"
