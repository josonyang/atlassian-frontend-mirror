{
	"name": "@atlaskit/popup",
	"version": "3.0.1",
	"description": "A popup displays brief content in an overlay.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx",
		"./experimental": "./src/entry-points/experimental/compositional.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx",
		"./experimental": "./src/entry-points/experimental/compositional.tsx"
	},
	"atlassian": {
		"team": "Design System Team",
		"runReact18": true,
		"website": {
			"name": "Popup",
			"category": "Overlays and layering"
		}
	},
	"dependencies": {
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/focus-ring": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/popper": "workspace:^",
		"@atlaskit/portal": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1",
		"bind-event-listener": "^3.0.0",
		"focus-trap": "^2.4.5",
		"memoize-one": "^6.0.0",
		"tiny-invariant": "^1.2.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/code": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/modal-dialog": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/textfield": "workspace:^",
		"@atlaskit/toggle": "workspace:^",
		"@atlaskit/visual-regression": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@compiled/react": "^0.18.3",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"ast-types": "^0.13.3",
		"jscodeshift": "^17.0.0",
		"raf-stub": "^2.0.1",
		"react-dom": "^18.2.0",
		"typescript": "~5.4.2"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"platform-feature-flags": {
		"platform_dst_popup-disable-focuslock": {
			"type": "boolean"
		},
		"platform-design-system-popup-ref": {
			"type": "boolean"
		},
		"design-system-closed-all-when-click-outside": {
			"type": "boolean"
		},
		"sibling-dropdown-close-issue": {
			"type": "boolean"
		},
		"platform-design-system-apply-popup-wrapper-focus": {
			"type": "boolean"
		},
		"fix-dropdown-close-outside-iframe": {
			"type": "boolean"
		},
		"popup-onclose-fix": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/popup/"
}
