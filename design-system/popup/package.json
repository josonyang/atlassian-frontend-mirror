{
	"name": "@atlaskit/popup",
	"version": "1.30.0",
	"description": "A popup displays brief content in an overlay.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx",
		"./experimental": "./src/entry-points/experimental/compositional.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx",
		"./experimental": "./src/entry-points/experimental/compositional.tsx"
	},
	"atlassian": {
		"team": "Design System Team",
		"runReact18": true,
		"website": {
			"name": "Popup",
			"category": "Overlays and layering"
		}
	},
	"dependencies": {
		"@atlaskit/css": "^0.7.0",
		"@atlaskit/ds-lib": "^3.3.0",
		"@atlaskit/focus-ring": "^2.0.0",
		"@atlaskit/layering": "^0.8.0",
		"@atlaskit/platform-feature-flags": "^0.3.0",
		"@atlaskit/popper": "^6.3.0",
		"@atlaskit/portal": "^4.9.0",
		"@atlaskit/primitives": "^13.3.0",
		"@atlaskit/theme": "^14.0.0",
		"@atlaskit/tokens": "^2.4.0",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1",
		"bind-event-listener": "^3.0.0",
		"focus-trap": "^2.4.5",
		"memoize-one": "^6.0.0",
		"tiny-invariant": "^1.2.0"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "*",
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/button": "^20.3.0",
		"@atlaskit/icon": "^23.1.0",
		"@atlaskit/ssr": "*",
		"@atlaskit/textfield": "^6.7.0",
		"@atlaskit/toggle": "^13.4.0",
		"@atlaskit/visual-regression": "*",
		"@atlassian/feature-flags-test-utils": "*",
		"@compiled/react": "^0.18.1",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/react": "^12.1.5",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"ast-types": "^0.13.3",
		"jscodeshift": "^0.13.0",
		"raf-stub": "^2.0.1",
		"react-dom": "^16.8.0",
		"storybook-addon-performance": "^0.17.3",
		"typescript": "~5.4.2",
		"wait-for-expect": "^1.2.0"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"platform-feature-flags": {
		"platform_dst_popup-disable-focuslock": {
			"type": "boolean"
		},
		"platform-design-system-dsp-20476-dropdown-menu": {
			"type": "boolean"
		},
		"design-system-closed-all-when-click-outside": {
			"type": "boolean"
		},
		"sibling-dropdown-close-issue": {
			"type": "boolean"
		},
		"platform-design-system-apply-popup-wrapper-focus": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/popup/"
}
