	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "*",
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/avatar": "^21.18.0",
		"@atlaskit/banner": "^12.7.0",
		"@atlaskit/button": "*",
