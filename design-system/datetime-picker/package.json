{
	"name": "@atlaskit/datetime-picker",
	"version": "17.0.17",
	"description": "A date time picker allows the user to select an associated date and time.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx"
	},
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Date time picker",
			"category": "Forms and input"
		}
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/calendar": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/locale": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/popper": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/select": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/visually-hidden": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*",
		"date-fns": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/code": "workspace:^",
		"@atlaskit/codemod-utils": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/modal-dialog": "workspace:^",
		"@atlaskit/popup": "workspace:^",
		"@atlaskit/range": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/textfield": "workspace:^",
		"@atlaskit/toggle": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "root:*",
		"@testing-library/user-event": "root:*",
		"@types/testing-library__jest-dom": "root:*",
		"jest-in-case": "root:*",
		"jscodeshift": "root:*",
		"moment": "root:*",
		"react-dom": "root:*",
		"react-lorem-component": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"styling": [
				"compiled",
				"static"
			],
			"ui-components": [
				"primitives",
				"lite-mode"
			],
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports"
		}
	},
	"platform-feature-flags": {
		"dst-date-picker-use-functional-component": {
			"type": "boolean"
		},
		"dst-date-time-picker-use-functional-component": {
			"type": "boolean"
		},
		"platform-visual-refresh-icons": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/datetime-picker/"
}
