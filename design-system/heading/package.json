{
	"name": "@atlaskit/heading",
	"version": "5.1.1",
	"description": "A heading is a typography component used to display text in different sizes and formats.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Heading",
			"category": "Text and data display",
			"status": {
				"type": "beta"
			}
		},
		"runReact18": true
	},
	"scripts": {
		"codegen": "ts-node ./scripts/codegen.tsx"
	},
	"dependencies": {
		"@atlaskit/css": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.3"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/formatting": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlassian/codegen": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "^13.4.0",
		"react-dom": "^18.2.0",
		"ts-node": "^10.9.1",
		"typescript": "~5.4.2"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions",
			"circular-dependencies": "file-and-folder-level"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"styling": [
				"static",
				"emotion"
			],
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports"
		}
	},
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"af:exports": {
		".": "./src/index.tsx"
	},
	"exports": {
		".": "./src/index.tsx"
	},
	"homepage": "https://atlassian.design/components/heading/",
	"platform-feature-flags": {
		"platform-typography-improved-color-control": {
			"type": "boolean"
		}
	}
}
