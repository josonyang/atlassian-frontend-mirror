{
  "name": "@atlaskit/ds-explorations",
  "version": "2.2.3",
  "description": "An experimental package for exploration and validation of spacing / typography foundations.",
  "license": "Apache-2.0",
  "atlassian": {
    "team": "Design System Team",
    "inPublicMirror": false,
    "releaseModel": "continuous"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
  "scripts": {
    "ak-postbuild": "cd ../../../ && yarn build @atlassian/codegen -d cjs,esm,es2019 && yarn build @atlaskit/tokens && cd packages/design-system/ds-explorations && yarn codegen-styles",
    "codegen-styles": "ts-node --project ../../../tsconfig.node.json ./scripts/codegen-styles",
    "prepare": "yarn ak-postbuild"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.tsx",
  "af:exports": {
    ".": "./src/index.tsx",
    "./box": "./src/components/box.partial.tsx",
    "./text": "./src/components/text.partial.tsx",
    "./stack": "./src/components/stack.partial.tsx",
    "./inline": "./src/components/inline.partial.tsx"
  },
  "dependencies": {
    "@atlaskit/tokens": "^1.9.0",
    "@babel/runtime": "^7.0.0",
    "@emotion/react": "^11.7.1",
    "tiny-invariant": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@atlaskit/badge": "*",
    "@atlaskit/button": "*",
    "@atlaskit/code": "*",
    "@atlaskit/docs": "*",
    "@atlaskit/ds-lib": "*",
    "@atlaskit/focus-ring": "*",
    "@atlaskit/heading": "*",
    "@atlaskit/icon": "*",
    "@atlaskit/lozenge": "*",
    "@atlaskit/primitives": "^0.12.0",
    "@atlaskit/section-message": "*",
    "@atlaskit/textfield": "*",
    "@atlaskit/visual-regression": "*",
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@atlassian/codegen": "^0.1.0",
    "@testing-library/react": "^12.1.5",
    "fs-extra": "^4.0.2",
    "prettier": "^2.8.0",
    "react-dom": "^16.8.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "wait-for-expect": "^1.2.0"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": "atlassian-conventions",
      "circular-dependencies": "file-and-folder-level"
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "design-system": "v1",
      "styling": [
        "static",
        "emotion"
      ],
      "ui-components": "lite-mode",
      "analytics": "analytics-next",
      "deprecation": "no-deprecated-imports"
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1",
  "homepage": "https://atlaskit.atlassian.com"
}
