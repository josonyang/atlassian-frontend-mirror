{
	"name": "@atlaskit/modal-dialog",
	"version": "14.6.0",
	"description": "A modal dialog displays content that requires user interaction, in a layer above the page.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Modal dialog",
			"category": "Messaging"
		}
	},
	"scripts": {
		"watch": "tsc --watch --noEmit --project './tsconfig.json'"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/blanket": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/focus-ring": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/motion": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/portal": "workspace:^",
		"@atlaskit/pragmatic-drag-and-drop": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*",
		"bind-event-listener": "root:*",
		"raf-schd": "root:*",
		"react-focus-lock": "root:*",
		"react-scrolllock": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/avatar": "workspace:^",
		"@atlaskit/avatar-group": "workspace:^",
		"@atlaskit/banner": "workspace:^",
		"@atlaskit/breadcrumbs": "workspace:^",
		"@atlaskit/checkbox": "workspace:^",
		"@atlaskit/code": "workspace:^",
		"@atlaskit/datetime-picker": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/dropdown-menu": "workspace:^",
		"@atlaskit/flag": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/popup": "workspace:^",
		"@atlaskit/radio": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/select": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/textfield": "workspace:^",
		"@atlaskit/tooltip": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/dom": "root:*",
		"@testing-library/react": "root:*",
		"@testing-library/user-event": "root:*",
		"@types/raf-schd": "root:*",
		"react-dom": "root:*",
		"react-lorem-component": "root:*",
		"react-sweet-state": "root:*",
		"tiny-invariant": "root:*",
		"typescript": "root:*"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"static",
				"emotion"
			]
		}
	},
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"exports": {
		".": "./src/index.tsx",
		"./types": "./src/types.tsx",
		"./full-screen": "./src/full-screen.tsx",
		"./hooks": "./src/hooks.tsx",
		"./modal-dialog": "./src/modal-dialog.tsx",
		"./modal-header": "./src/modal-header.tsx",
		"./modal-title": "./src/modal-title.tsx",
		"./modal-body": "./src/modal-body.tsx",
		"./modal-footer": "./src/modal-footer.tsx",
		"./modal-transition": "./src/modal-transition.tsx"
	},
	"platform-feature-flags": {
		"platform_modal-dialog-heading-icon-a11y-fix": {
			"type": "boolean"
		},
		"platform_ads_explicit_font_styles": {
			"type": "boolean"
		},
		"platform-dst-open-layer-observer-layer-type": {
			"type": "boolean"
		}
	},
	"homepage": "https://atlassian.design/components/modal-dialog/"
}
