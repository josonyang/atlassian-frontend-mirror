{
	"name": "@atlaskit/atlassian-navigation",
	"version": "5.0.0",
	"description": "A horizontal navigation component for Atlassian products.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./skeleton": "./src/skeleton.tsx",
		"./skeleton-help-button": "./src/skeleton-help-button.tsx",
		"./skeleton-settings-button": "./src/skeleton-settings-button.tsx",
		"./skeleton-notification-button": "./src/skeleton-notification-button.tsx",
		"./skeleton-switcher-button": "./src/skeleton-switcher-button.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./skeleton": "./src/skeleton.tsx",
		"./skeleton-help-button": "./src/skeleton-help-button.tsx",
		"./skeleton-settings-button": "./src/skeleton-settings-button.tsx",
		"./skeleton-notification-button": "./src/skeleton-notification-button.tsx",
		"./skeleton-switcher-button": "./src/skeleton-switcher-button.tsx"
	},
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Atlassian navigation",
			"category": "Navigation",
			"status": {
				"type": "intent-to-deprecate",
				"description": "We are in the process of building a new top navigation component: [@atlassian/navigation-system](https://staging.atlassian.design/components/navigation-system/top-navigation/examples) (For Atlassian employees only at this stage)"
			}
		},
		"runReact18": true
	},
	"dependencies": {
		"@atlaskit/analytics-namespaced-context": "^7.0.0",
		"@atlaskit/button": "^21.1.0",
		"@atlaskit/ds-lib": "^4.0.0",
		"@atlaskit/icon": "^24.1.0",
		"@atlaskit/logo": "*",
		"@atlaskit/platform-feature-flags": "^1.1.0",
		"@atlaskit/popup": "^2.0.0",
		"@atlaskit/primitives": "^14.1.0",
		"@atlaskit/theme": "^17.0.0",
		"@atlaskit/tokens": "^4.3.0",
		"@atlaskit/tooltip": "^20.0.0",
		"@atlaskit/width-detector": "^5.0.0",
		"@babel/runtime": "^7.0.0",
		"@emotion/react": "^11.7.1",
		"chromatism": "^2.6.0",
		"lodash": "^4.17.21"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "*",
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/analytics-next": "^11.0.0",
		"@atlaskit/atlassian-notifications": "^0.7.0",
		"@atlaskit/avatar": "^24.0.0",
		"@atlaskit/badge": "^18.0.0",
		"@atlaskit/docs": "*",
		"@atlaskit/drawer": "^10.0.0",
		"@atlaskit/dropdown-menu": "^13.0.0",
		"@atlaskit/form": "^12.0.0",
		"@atlaskit/menu": "^3.1.0",
		"@atlaskit/notification-indicator": "*",
		"@atlaskit/notification-log-client": "*",
		"@atlaskit/section-message": "^8.0.0",
		"@atlaskit/spinner": "^18.0.0",
		"@atlaskit/textfield": "^8.0.0",
		"@atlaskit/visual-regression": "*",
		"@atlassian/switcher": "^35.3.0",
		"@atlassian/switcher-test-utils": "^4.1.0",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"jest-emotion": "^10.0.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-intl-next": "npm:react-intl@^5.18.1",
		"typescript": "~5.4.2"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"design-system": "v1",
			"dom-events": "use-bind-event-listener",
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"styling": [
				"emotion",
				"static"
			],
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports"
		}
	},
	"homepage": "https://atlassian.design/components/atlassian-navigation/",
	"platform-feature-flags": {
		"platform-component-visual-refresh": {
			"type": "boolean"
		}
	}
}
