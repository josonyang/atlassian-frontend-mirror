{
	"name": "@atlaskit/drawer",
	"version": "10.1.1",
	"description": "A drawer is a panel that slides in from the left side of the screen.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./compiled": "./src/entry-points/compiled.tsx",
		"./constants": "./src/constants.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./compiled": "./src/entry-points/compiled.tsx",
		"./constants": "./src/constants.tsx"
	},
	"homepage": "https://atlassian.design/components/drawer",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Drawer",
			"category": "Overlays and layering",
			"subPages": [
				{
					"title": "Drawer (legacy)",
					"id": "drawer-legacy",
					"status": {
						"type": "intent-to-deprecate",
						"description": "Legacy drawer will soon be deprecated. Please use the new [drawer](/components/drawer).",
						"actions": [
							{
								"text": "View the migration guide",
								"href": "/components/drawer/migration-guide"
							}
						]
					},
					"sortKey": 6
				}
			]
		},
		"runReact18": true
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/blanket": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/motion": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/portal": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.3",
		"@emotion/react": "^11.7.1",
		"bind-event-listener": "^3.0.0",
		"exenv": "^1.2.2",
		"react-focus-lock": "^2.9.5",
		"react-scrolllock": "^5.0.1",
		"tiny-invariant": "^1.2.0",
		"use-callback-ref": "^1.2.3"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/code": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/dropdown-menu": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/form": "workspace:^",
		"@atlaskit/inline-message": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/menu": "workspace:^",
		"@atlaskit/modal-dialog": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/exenv": "^1.2.0",
		"react-dom": "^18.2.0",
		"react-lorem-component": "^0.13.0",
		"typescript": "~5.4.2"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"styling": [
				"static",
				"emotion"
			],
			"deprecation": "no-deprecated-imports",
			"ui-components": [
				"primitives"
			]
		}
	}
}
