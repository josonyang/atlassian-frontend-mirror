{
	"name": "@atlaskit/drawer",
	"version": "9.4.0",
	"description": "A drawer is a panel that slides in from the left side of the screen.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"af:exports": {
		".": "./src/index.tsx",
		"./compiled": "./src/entry-points/compiled.tsx",
		"./constants": "./src/constants.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./compiled": "./src/entry-points/compiled.tsx",
		"./constants": "./src/constants.tsx"
	},
	"homepage": "https://atlassian.design/components/drawer",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Drawer",
			"category": "Overlays and layering",
			"subPages": [
				{
					"title": "Drawer (legacy)",
					"id": "drawer-legacy",
					"status": {
						"type": "intent-to-deprecate",
						"description": "Legacy drawer will soon be deprecated. Please use the new [drawer](/components/drawer).",
						"actions": [
							{
								"text": "View the migration guide",
								"href": "/components/drawer/migration-guide"
							}
						]
					},
					"sortKey": 6
				}
			]
		},
		"runReact18": true
	},
	"dependencies": {
		"@atlaskit/analytics-next": "^10.3.0",
		"@atlaskit/blanket": "^14.1.0",
		"@atlaskit/button": "^20.4.0",
		"@atlaskit/css": "^0.8.0",
		"@atlaskit/icon": "^23.5.0",
		"@atlaskit/layering": "^1.1.0",
		"@atlaskit/motion": "^1.10.0",
		"@atlaskit/platform-feature-flags": "^0.3.0",
		"@atlaskit/portal": "^4.11.0",
		"@atlaskit/theme": "^14.1.0",
		"@atlaskit/tokens": "^3.3.0",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.1",
		"@emotion/react": "^11.7.1",
		"bind-event-listener": "^3.0.0",
		"exenv": "^1.2.2",
		"react-focus-lock": "^2.9.5",
		"react-scrolllock": "^5.0.1",
		"tiny-invariant": "^1.2.0",
		"use-callback-ref": "^1.2.3"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "*",
		"@af/integration-testing": "*",
		"@af/visual-regression": "*",
		"@atlaskit/code": "^15.7.0",
		"@atlaskit/docs": "*",
		"@atlaskit/dropdown-menu": "^12.26.0",
		"@atlaskit/ds-lib": "^3.5.0",
		"@atlaskit/form": "^11.1.0",
		"@atlaskit/inline-message": "^13.1.0",
		"@atlaskit/link": "*",
		"@atlaskit/menu": "^2.14.0",
		"@atlaskit/modal-dialog": "^12.20.0",
		"@atlaskit/primitives": "^13.4.0",
		"@atlaskit/section-message": "*",
		"@atlaskit/ssr": "*",
		"@atlaskit/visual-regression": "*",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/exenv": "^1.2.0",
		"react-dom": "^18.2.0",
		"react-lorem-component": "^0.13.0",
		"typescript": "~5.4.2"
	},
	"platform-feature-flags": {
		"platform-visual-refresh-icon-ads-migration": {
			"type": "boolean"
		}
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"styling": [
				"static",
				"emotion"
			],
			"deprecation": "no-deprecated-imports",
			"ui-components": [
				"primitives"
			]
		}
	}
}
