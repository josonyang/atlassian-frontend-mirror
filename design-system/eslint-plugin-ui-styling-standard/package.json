{
  "name": "@atlaskit/eslint-plugin-ui-styling-standard",
  "version": "0.15.1",
  "author": "Atlassian Pty Ltd",
  "description": "The eslint plugin to enforce and educate on Atlassian's UI Styling Standard",
  "license": "Apache-2.0",
  "atlassian": {
    "team": "Design System Team",
    "inPublicMirror": true,
    "releaseModel": "continuous",
    "website": {
      "name": "UI Styling Standard",
      "category": "Tooling"
    },
    "productPushConsumption": [
      "jira"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://atlassian.design/components/eslint-plugin-ui-styling-standard",
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.tsx",
  "af:exports": {
    ".": "./src/index.tsx"
  },
  "exports": {
    ".": "./index.js"
  },
  "dependencies": {
    "@atlaskit/eslint-utils": "^1.5.0",
    "@babel/runtime": "^7.0.0",
    "@typescript-eslint/utils": "^5.48.1",
    "ajv": "^6.12.6",
    "eslint-codemod-utils": "^1.8.6",
    "eslint-scope": "^8.0.0",
    "esquery": "^1.5.0",
    "estraverse": "^5.3.0",
    "json-schema-to-ts": "^3.1.0",
    "outdent": "^0.5.0",
    "postcss-selector-parser": "^6.0.16"
  },
  "devDependencies": {
    "@af/formatting": "*",
    "@atlaskit/ds-lib": "^2.3.0",
    "@atlaskit/eslint-plugin-design-system": "^10.4.0",
    "@atlassian/codegen": "*",
    "@compiled/eslint-plugin": "^0.13.6",
    "@types/eslint": "^8.56.6",
    "@types/estraverse": "^5.1.7",
    "eslint": "^8.49.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.4.2"
  },
  "scripts": {
    "codegen": "ts-node -r tsconfig-paths/register ./scripts/codegen.tsx"
  }
}
