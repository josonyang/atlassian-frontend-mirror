{
  "name": "@atlaskit/eslint-plugin-ui-styling-standard",
  "version": "0.2.1",
  "author": "Atlassian Pty Ltd",
  "description": "The eslint plugin to enforce and educate on Atlassian's UI Styling Standard",
  "license": "UNLICENSED",
  "atlassian": {
    "team": "Design System Team",
    "inPublicMirror": true,
    "releaseModel": "continuous",
    "website": {
      "name": "UI Styling Standard",
      "category": "Tooling"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://atlassian.design/components/eslint-plugin-ui-styling-standard",
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.tsx",
  "af:exports": {
    ".": "./src/index.tsx"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@atlaskit/eslint-plugin-design-system": "^8.27.0",
    "@atlassian/atlassian-frontend-prettier-config-1.0.1": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.1",
    "@compiled/eslint-plugin": "^0.13.3",
    "@types/eslint": "^8.4.5",
    "eslint": "^7.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.9.5"
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.1"
}
