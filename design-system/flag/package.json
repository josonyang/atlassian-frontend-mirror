{
	"name": "@atlaskit/flag",
	"version": "17.3.2",
	"description": "A flag is used for confirmations, alerts, and acknowledgments that require minimal user interaction, often displayed using a flag group.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Flag",
			"category": "Messaging"
		}
	},
	"af:exports": {
		".": "./src/index.tsx",
		"./auto-dismiss-flag": "./src/auto-dismiss-flag.tsx",
		"./constants": "./src/constants.tsx",
		"./flag-actions": "./src/flag-actions.tsx",
		"./flag-group": "./src/flag-group.tsx",
		"./flag-provider": "./src/flag-provider.tsx",
		"./flag": "./src/flag.tsx",
		"./theme": "./src/theme.tsx",
		"./types": "./src/types.tsx"
	},
	"exports": {
		".": "./src/index.tsx",
		"./auto-dismiss-flag": "./src/auto-dismiss-flag.tsx",
		"./constants": "./src/constants.tsx",
		"./flag-actions": "./src/flag-actions.tsx",
		"./flag-group": "./src/flag-group.tsx",
		"./flag-provider": "./src/flag-provider.tsx",
		"./flag": "./src/flag.tsx",
		"./theme": "./src/theme.tsx",
		"./types": "./src/types.tsx"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/motion": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/portal": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/visually-hidden": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/drawer": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/radio": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/spinner": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/react": "root:*",
		"react-dom": "root:*"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"design-system": "v1",
			"dom-events": "use-bind-event-listener",
			"styling": [
				"static",
				"emotion"
			],
			"ui-components": [
				"primitives",
				"lite-mode"
			],
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports"
		}
	},
	"homepage": "https://atlassian.design/components/flag/",
	"platform-feature-flags": {
		"platform_ads_explicit_font_styles": {
			"type": "boolean"
		}
	}
