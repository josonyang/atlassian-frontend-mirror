{
	"name": "@atlaskit/storybook-addon-design-system",
	"version": "3.1.4",
	"description": "Design token storybook addon",
	"author": "Atlassian Pty Ltd",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Storybook addon",
			"category": "Tooling"
		}
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"type": "module",
	"main": "src/index.tsx",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"exports": {
		".": "./src/index.tsx",
		"./preview": {
			"publish": null,
			"default": "./preview.js"
		},
		"./preset": {
			"publish": null,
			"default": "./preset.js"
		},
		"./manager": {
			"publish": null,
			"default": "./manager.js"
		},
		"./package.json": {
			"publish": null,
			"default": "./package.json"
		}
	},
	"dependencies": {
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@storybook/components": "^8.4.7",
		"@storybook/icons": "root:*",
		"@storybook/manager-api": "root:*",
		"@storybook/preview-api": "root:*",
		"@storybook/types": "root:*",
		"storybook": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"bundler": {
		"exportEntries": [
			"src/index.tsx"
		],
		"managerEntries": [
			"src/manager.tsx"
		],
		"previewEntries": [
			"src/preview.tsx"
		],
		"nodeEntries": [
			"src/preset.tsx"
		]
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports",
			"styling": [
				"emotion"
			]
		}
	},
	"homepage": "https://atlassian.design/components/storybook-addon-design-system"
}
