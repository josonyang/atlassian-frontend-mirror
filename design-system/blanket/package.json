{
	"name": "@atlaskit/blanket",
	"version": "15.0.16",
	"description": "A blanket covers the underlying UI for a layered component, such as a modal dialog or a tooltip.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"**/*.compiled.css"
	],
	"atlaskit:src": "src/index.tsx",
	"homepage": "https://atlassian.design/components/blanket",
	"atlassian": {
		"team": "Design System Team",
		"website": {
			"name": "Blanket",
			"category": "Overlays and layering"
		}
	},
	"exports": {
		".": "./src/index.tsx",
		"./types": "./src/entry-points/types.tsx"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/codemod-utils": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/ds-lib": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"@compiled/react": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@af/accessibility-testing": "workspace:^",
		"@af/integration-testing": "workspace:^",
		"@atlaskit/badge": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/docs": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/layering": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/section-message": "workspace:^",
		"@atlaskit/toggle": "workspace:^",
		"@atlassian/ssr-tests": "workspace:^",
		"@testing-library/dom": "root:*",
		"@testing-library/react": "root:*",
		"@testing-library/user-event": "root:*",
		"bind-event-listener": "root:*",
		"jscodeshift": "root:*",
		"react-dom": "root:*",
		"react-lorem-component": "root:*",
		"storybook-addon-performance": "root:*"
	},
	"keywords": [
		"atlaskit",
		"react",
		"ui"
	],
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		},
		"@repo/internal": {
			"dom-events": "use-bind-event-listener",
			"design-system": "v1",
			"styling": [
				"static",
				"emotion"
			],
			"ui-components": "lite-mode",
			"analytics": "analytics-next",
			"design-tokens": [
				"color",
				"spacing"
			],
			"deprecation": "no-deprecated-imports"
		}
	}
}
