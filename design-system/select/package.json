		"./types": "./src/types.tsx",
		".": "./src/index.tsx"
	},
	"exports": {
		"./AsyncSelect": "./src/entry-points/async-select.tsx",
		"./Select": "./src/entry-points/select.tsx",
		"./AsyncCreatableSelect": "./src/entry-points/async-creatable-select.tsx",
		"./CreatableSelect": "./src/entry-points/creatable-select.tsx",
		"./types": "./src/types.tsx",
		".": "./src/index.tsx"
	},
