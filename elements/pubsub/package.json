{
	"name": "@atlaskit/pubsub",
	"version": "7.0.0",
	"description": "Interacts with Atlassian Frontend PubSub service to receive pubsub events.",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"atlaskit:src": "src/index.ts",
	"af:exports": {
		".": "./src/index.ts",
		"./apiTypes": "./src/apiTypes.ts",
		"./client": "./src/client.ts",
		"./featureFlags": "./src/featureFlags.ts",
		"./types": "./src/types.ts"
	},
	"exports": {
		".": "./src/index.ts",
		"./apiTypes": "./src/apiTypes.ts",
		"./client": "./src/client.ts",
		"./featureFlags": "./src/featureFlags.ts",
		"./types": "./src/types.ts"
	},
	"atlassian": {
		"team": "Event Platform",
		"website": {
			"name": "pubsub"
		}
	},
	"config": {
		"access": "public"
	},
	"dependencies": {
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/util-service-support": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"eventemitter2": "^4.1.0",
		"exponential-backoff": "^3.1.1"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"es6-promise": "^4.0.5",
		"fetch-mock": "^8.0.0",
		"jest-extended": "^4.0.2",
		"sinon": "^2.2.0",
		"typescript": "~5.4.2"
	},
	"platform-feature-flags": {
		"platform-delay-retries-for-clients-on-fps": {
			"type": "boolean"
		},
		"platform-delay-retries-on-first-connect-fps": {
			"type": "boolean"
		}
	}
}
