{
	"name": "@atlaskit/emoji",
	"version": "69.0.10",
	"description": "Fabric emoji React components",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"atlaskit:src": "src/index.ts",
	"atlassian": {
		"team": "Editor: Collaboration",
		"website": {
			"name": "Emoji"
		},
		"runReact18": true
	},
	"config": {
		"access": "public"
	},
	"dependencies": {
		"@atlaskit/analytics-next": "workspace:^",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/css": "workspace:^",
		"@atlaskit/heading": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/media-client": "workspace:^",
		"@atlaskit/media-client-react": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/platform-feature-flags-react": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/spinner": "workspace:^",
		"@atlaskit/textfield": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@atlaskit/tooltip": "workspace:^",
		"@atlaskit/ufo": "workspace:^",
		"@atlaskit/util-service-support": "workspace:^",
		"@atlaskit/visually-hidden": "workspace:^",
		"@babel/runtime": "^7.0.0",
		"@compiled/react": "^0.18.3",
		"@tanstack/react-virtual": "3.0.0-beta.60",
		"js-search": "^2.0.0",
		"lru_map": "^0.4.1",
		"react-focus-lock": "^2.9.5",
		"react-intersection-observer": "^8.26.2",
		"react-loadable": "^5.1.0",
		"use-debounce": "^3.4.0",
		"uuid": "^3.1.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-intl-next": "npm:react-intl@^5.18.1"
	},
	"devDependencies": {
		"@af/integration-testing": "workspace:^",
		"@af/visual-regression": "workspace:^",
		"@atlaskit/elements-test-helpers": "workspace:^",
		"@atlaskit/media-core": "workspace:^",
		"@atlaskit/ssr": "workspace:^",
		"@atlaskit/visual-regression": "workspace:^",
		"@atlassian/feature-flags-test-utils": "workspace:^",
		"@emotion/jest": "^11.8.0",
		"@testing-library/dom": "^10.1.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.3",
		"@types/js-search": "^1.4.0",
		"dotenv": "^8.2.0",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"es6-promise": "^4.0.5",
		"fetch-mock": "^8.0.0",
		"jest-axe": "^8.0.0",
