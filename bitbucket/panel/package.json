{
	"name": "@atlaskit/panel",
	"version": "0.11.22",
	"description": "A React component rendering a collapsible panel",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://bitbucket.org/atlassian/atlassian-frontend-mirror",
	"author": "Atlassian Pty Ltd",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"module:es2019": "dist/es2019/index.js",
	"types": "dist/types/index.d.ts",
	"typesVersions": {
		">=4.5 <4.9": {
			"*": [
				"dist/types-ts4.5/*",
				"dist/types-ts4.5/index.d.ts"
			]
		}
	},
	"exports": {
		".": "./src/index.ts",
		"./i18n/*": {
			"publish": null,
			"default": "./src/i18n/*.ts"
		}
	},
	"atlaskit:src": "src/index.ts",
	"atlassian": {
		"team": "Bitbucket: CoreX",
		"website": {
			"name": "Panel"
		}
	},
	"dependencies": {
		"@atlaskit/afm-i18n-platform-bitbucket-panel": "root:*",
		"@atlaskit/button": "workspace:^",
		"@atlaskit/icon": "workspace:^",
		"@atlaskit/link": "workspace:^",
		"@atlaskit/platform-feature-flags": "workspace:^",
		"@atlaskit/primitives": "workspace:^",
		"@atlaskit/theme": "workspace:^",
		"@atlaskit/tokens": "workspace:^",
		"@babel/runtime": "root:*",
		"react-animate-height": "root:*",
		"react-focus-within": "root:*",
		"react-intl-next": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"styled-components": "^3.2.6"
	},
	"devDependencies": {
		"@testing-library/react": "root:*",
		"@testing-library/user-event": "root:*"
	},
	"techstack": {
		"@atlassian/frontend": {
			"import-structure": "atlassian-conventions"
		}
	},
	"sideEffects": [
		"*.compiled.css"
	],
	"platform-feature-flags": {
		"dst-a11y__replace-anchor-with-link__bitbucket-core": {
			"type": "boolean"
		}
	}
}
