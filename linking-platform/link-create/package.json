{
  "name": "@atlaskit/link-create",
  "version": "1.8.0",
  "description": "The driver component of meta creation flow",
  "author": "Atlassian Pty Ltd",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "atlassian": {
    "team": "Linking Platform",
    "inPublicMirror": false,
    "releaseModel": "continuous",
    "website": {
      "name": "LinkCreate"
    }
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "af:exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@atlaskit/analytics-next": "^9.1.0",
    "@atlaskit/button": "^16.14.0",
    "@atlaskit/empty-state": "^7.6.0",
    "@atlaskit/form": "^9.0.0",
    "@atlaskit/icon": "^21.12.0",
    "@atlaskit/intl-messages-provider": "^1.0.0",
    "@atlaskit/linking-common": "^4.17.0",
    "@atlaskit/modal-dialog": "^12.8.0",
    "@atlaskit/platform-feature-flags": "^0.2.1",
    "@atlaskit/primitives": "^1.10.0",
    "@atlaskit/select": "^16.7.0",
    "@atlaskit/spinner": "^15.6.0",
    "@atlaskit/textfield": "^5.6.0",
    "@atlaskit/tokens": "^1.28.0",
    "@babel/runtime": "^7.0.0",
    "@emotion/react": "^11.7.1",
    "debounce-promise": "^3.1.2",
    "final-form": "^4.20.1",
    "react-final-form": "^6.5.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-intl-next": "npm:react-intl@^5.18.1"
  },
  "devDependencies": {
    "@af/visual-regression": "*",
    "@atlaskit/link-test-helpers": "^6.2.0",
    "@atlaskit/visual-regression": "*",
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@atlassian/feature-flags-test-utils": "*",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/debounce-promise": "^3.1.2",
    "fetch-mock": "^8.0.0",
    "react-dom": "^16.8.0",
    "typescript": "~4.9.5",
    "wait-for-expect": "^1.2.0"
  },
  "scripts": {
    "codegen-analytics": "yarn run ts-analytics-codegen --command='yarn workspace @atlaskit/link-create run codegen-analytics'"
  },
  "techstack": {
    "@atlassian/frontend": {
      "code-structure": [
        "tangerine-next"
      ],
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "analytics": [
        "analytics-next"
      ],
      "theming": [
        "react-context",
        "tokens"
      ],
      "ui-components": [
        "lite-mode"
      ],
      "deprecation": [
        "no-deprecated-imports"
      ],
      "styling": [
        "static",
        "emotion"
      ],
      "imports": [
        "import-no-extraneous-disable-for-examples-and-docs"
      ]
    }
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0",
  "platform-feature-flags": {
    "platform.linking-platform.link-create.enable-edit": {
      "type": "boolean"
    },
    "platform.linking-platform.link-create.confirm-dismiss-dialog": {
      "type": "boolean"
    },
    "platform.linking-platform.link-create.outer-error-boundary": {
      "type": "boolean"
    },
    "platform.linking-platform.link-create.enable-sentry-client": {
      "type": "boolean"
    }
  }
}
