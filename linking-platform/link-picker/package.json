{
  "name": "@atlaskit/link-picker",
  "version": "1.30.1",
  "description": "Standalone link picker",
  "author": "Atlassian Pty Ltd",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "atlassian": {
    "team": "Linking Platform",
    "inPublicMirror": false,
    "releaseModel": "continuous",
    "website": {
      "name": "Link Picker"
    }
  },
  "repository": "https://bitbucket.org/atlassian/atlassian-frontend",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "af:exports": {
    ".": "./src/index.ts"
  },
  "platform-feature-flags": {
    "platform.linking-platform.link-picker.enable-empty-state": {
      "type": "boolean"
    },
    "platform.linking-platform.link-picker.lazy-intl-messages": {
      "type": "boolean"
    },
    "platform.linking-platform.link-picker.fixed-height-search-results": {
      "type": "boolean"
    },
    "platform.linking-platform.link-picker.enable-jira-create": {
      "type": "boolean"
    }
  },
  "scripts": {
    "ak-postbuild": "ls -d dist/* | grep -v dist/types | xargs -n 1 copyfiles -u 1 -V src/**/*.svg",
    "codegen-analytics": "yarn run ts-analytics-codegen --command='yarn workspace @atlaskit/link-picker run codegen-analytics'"
  },
  "dependencies": {
    "@atlaskit/analytics-next": "^9.1.0",
    "@atlaskit/button": "^16.12.0",
    "@atlaskit/empty-state": "^7.6.0",
    "@atlaskit/form": "^8.11.0",
    "@atlaskit/frontend-utilities": "^2.7.0",
    "@atlaskit/icon": "^21.12.0",
    "@atlaskit/linking-common": "^4.13.0",
    "@atlaskit/onboarding": "^10.8.2",
    "@atlaskit/platform-feature-flags": "^0.2.1",
    "@atlaskit/spinner": "^15.6.0",
    "@atlaskit/tabs": "^13.4.0",
    "@atlaskit/textfield": "^5.6.0",
    "@atlaskit/theme": "^12.6.0",
    "@atlaskit/tokens": "^1.28.0",
    "@atlaskit/tooltip": "^17.8.0",
    "@atlaskit/ufo": "^0.2.0",
    "@atlaskit/visually-hidden": "^1.2.0",
    "@babel/runtime": "^7.0.0",
    "@emotion/react": "^11.7.1",
    "@react-loosely-lazy/manifest": "^1.0.0",
    "date-fns": "^2.17.0",
    "react-loosely-lazy": "^1.0.0",
    "use-debounce": "^3.4.0",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-intl-next": "npm:react-intl@^5.18.1"
  },
  "devDependencies": {
    "@af/analytics-codegen": "^0.1.0",
    "@af/visual-regression": "*",
    "@atlaskit/dropdown-menu": "^12.1.0",
    "@atlaskit/link-provider": "^1.6.0",
    "@atlaskit/link-test-helpers": "^6.2.0",
    "@atlaskit/visual-regression": "*",
    "@atlaskit/webdriver-runner": "*",
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@atlassian/feature-flags-test-utils": "*",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "fetch-mock": "^8.0.0",
    "jest-extended": "^0.11.2",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "node-fetch": "^2.6.7",
    "prettier": "^2.8.0",
    "react-dom": "^16.8.0",
    "sinon": "^2.2.0",
    "typescript": "~4.9.5"
  },
  "techstack": {
    "@atlassian/frontend": {
      "code-structure": [
        "tangerine-next"
      ],
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "analytics": [
        "analytics-next"
      ],
      "design-tokens": [
        "color",
        "spacing"
      ],
      "imports": [
        "import-no-extraneous-disable-for-examples-and-docs"
      ]
    }
  },
  "techstackIgnore": {
    "stricter": [
      "@atlassian/tangerine/project-structure"
    ]
  },
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0"
}
