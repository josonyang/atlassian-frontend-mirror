		"react-loosely-lazy": "root:*",
		"use-callback-ref": "root:*",
		"use-debounce": "root:*",
		"uuid": "root:*"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-intl-next": "*"
	},
	"devDependencies": {
		"@af/analytics-codegen": "workspace:^",
