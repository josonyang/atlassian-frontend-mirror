{
  "name": "@atlaskit/pragmatic-drag-and-drop-docs",
  "version": "1.0.7",
  "description": "This package holds the documentation for Pragmatic drag and drop in one place. It is not intended to be consumed directly by consumers. The package is used by other tools for sharing examples",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "atlassian": {
    "team": "Design System Team",
    "inPublicMirror": true,
    "releaseModel": "continuous",
    "website": {
      "name": "Pragmatic drag and drop",
      "category": "Libraries"
    }
  },
  "repository": "https://github.com/atlassian/pragmatic-drag-and-drop",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "module:es2019": "dist/es2019/index.js",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    ">=4.5 <4.9": {
      "*": [
        "dist/types-ts4.5/*",
        "dist/types-ts4.5/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "atlaskit:src": "src/index.ts",
  "af:exports": {
    ".": "./src/index.ts"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@atlaskit/avatar": "^21.5.0",
    "@atlaskit/badge": "^16.0.0",
    "@atlaskit/button": "^17.12.0",
    "@atlaskit/code": "^15.1.0",
    "@atlaskit/focus-ring": "^1.2.0",
    "@atlaskit/grid": "^0.12.0",
    "@atlaskit/heading": "^2.1.0",
    "@atlaskit/icon": "^22.1.0",
    "@atlaskit/pragmatic-drag-and-drop": "^1.1.0",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "^1.0.0",
    "@atlaskit/pragmatic-drag-and-drop-react-beautiful-dnd-autoscroll": "^1.1.0",
    "@atlaskit/pragmatic-drag-and-drop-react-drop-indicator": "^1.1.0",
    "@atlaskit/primitives": "^5.5.0",
    "@atlaskit/theme": "^12.7.0",
    "@atlaskit/tokens": "^1.43.0",
    "@atlaskit/visual-regression": "*",
    "@atlassian/atlassian-frontend-prettier-config-1.0.0": "npm:@atlassian/atlassian-frontend-prettier-config@1.0.0",
    "@emotion/react": "^11.7.1",
    "bind-event-listener": "^2.1.1",
    "memoize-one": "^6.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "tiny-invariant": "^1.2.0",
    "typescript": "~5.4.2"
  },
  "techstack": {
    "@atlassian/frontend": {
      "import-structure": [
        "atlassian-conventions"
      ],
      "circular-dependencies": [
        "file-and-folder-level"
      ]
    },
    "@repo/internal": {
      "dom-events": "use-bind-event-listener",
      "ui-components": "lite-mode",
      "analytics": "analytics-next",
      "design-tokens": [
        "color"
      ],
      "deprecation": "no-deprecated-imports",
      "styling": [
        "emotion",
        "static"
      ]
    }
  },
  "homepage": "https://atlassian.design/components/pragmatic-drag-and-drop/",
  "prettier": "@atlassian/atlassian-frontend-prettier-config-1.0.0"
}
